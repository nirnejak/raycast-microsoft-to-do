{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "microsoft-to-do",
  "title": "Microsoft To do",
  "description": "",
  "icon": "command-icon.png",
  "author": "nirnejak",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Task",
      "description": "Create a Task on Microsoft Todo",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "Microsoft Graph API Token",
      "description": "Microsoft Graph API Token",
      "link": "https://developer.microsoft.com/en-us/graph/",
      "placeholder": "Microsoft Graph API Token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "build-old": "ray build -e dist",
    "build": "ray build -o dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
